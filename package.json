{
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "commit": "git-cz"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": false
    },
    "name": "countdown",
    "author": "wuhan01",
    "module": "dist/countdown.esm.js",
    "size-limit": [
        {
            "path": "dist/countdown.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/countdown.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^7.0.4",
        "size-limit": "^7.0.8",
        "standard-version": "^9.3.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@types/lodash.isequal": "^4.5.5",
        "lodash.isequal": "^4.5.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
